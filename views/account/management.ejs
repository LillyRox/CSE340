

<main>
  <h1>Account Management</h1>
  <p>You're logged in.</p>

  <% if (message && message.length > 0) { %>
    <div class="notice"><%= message %></div>
  <% } %>

  <% if (errors && errors.length > 0) { %>
    <div class="errors">
      <ul>
        <% errors.forEach(error => { %>
          <li><%= error.msg %></li>
        <% }) %>
      </ul>
    </div>
  <% } %>
</main>

